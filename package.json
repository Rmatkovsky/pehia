{
  "name": "pehia",
  "version": "1.0.0",
  "description": "Pehia",
  "private": true,
  "main": "app.js",
  "scripts": {
    "build": "npm run build:frontend && rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./frontend,./webpack --copy-files",
    "start": "npm run build && node dist/app.js",
    "build:frontend": "npm run develop:build",
    "prod:build": "npm i && cross-env BUILD=true NODE_ENV=production webpack -p --progress --config ./webpack/webpack.config.production.babel.js",
    "develop:build": "cross-env BUILD=true NODE_ENV=development webpack -d --progress --config ./webpack/webpack.config.develop.babel.js",
    "develop:devserver": "cross-env BUILD_ENV=local NODE_ENV=development webpack-dev-server --progress --config ./webpack/webpack.config.develop.babel.js",
    "lint": "eslint develop test --ext .jsx,.js --fix --cache --ignore-pattern .gitignore",
    "clean": "rimraf public/ node_modules/ test_reports/",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
